                $(".category").children('a').click(function (index) {
                    var path = 'sandwich';
                    var data = $(this).attr("id");

                    var param = {"path" : path, "category" : data};
                    $.ajax({
                        type: "POST",
                        url: "/getCategoryMenu",
                        data: param,
                        cache: false,
                        success: function (result) {
                            if(result != "") {
                                location.href = "/menuView/" + path + "?menuItemIdx=" + result;
                            }else{
                                alert('해당 제품이 없습니다.');
                            }
                        }
                    });
                });
