{
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/sitemap.xml",
        "use": "@vercel/static"
      },
      {
        "src": "public/robots.txt",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      },
      {
        "src": "/sitemap.xml",
        "dest": "/sitemap.xml"
      },
      {
        "src": "/robots.txt",
        "dest": "/robots.txt"
      }
    ]
  }
  